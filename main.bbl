      \field{journaltitle}{Nature Communications}
      \field{month}{12}
      \field{number}{1}
      \field{title}{{Twin boundary migration in an individual platinum nanocrystal during catalytic CO oxidation}}
      \field{volume}{12}
      \field{year}{2021}
